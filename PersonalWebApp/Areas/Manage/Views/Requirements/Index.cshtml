@model IEnumerable<PersonalWebApp.Models.Science>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Manage/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create" asp-controller="Requirements">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfilId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfilId)
            </td>
            <td>
                    <a asp-action="Update" asp-controller="Requirements"  asp-route-Id="@item.Id">Edit</a>
            </td>
            <td>
                    <a asp-action="Delete" asp-controller="Requirements"  asp-route-Id="@item.Id">Delete</a>
            </td>
           
        </tr>
}
    </tbody>
</table>
